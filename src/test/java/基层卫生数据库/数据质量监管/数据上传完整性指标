where not exists(select 1 from CHILDREN_ABNOR_MALREACTION a where a.INOCULATION_SERIAL_NUMBER=t.INOCULATION_SERIAL_NUMBER and a.RECORD_INSTITUTE_CODE=t.INOCULATION_INSTITUTION_CODE)



