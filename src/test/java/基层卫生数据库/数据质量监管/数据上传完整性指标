where not exists(select 1 from DIABETIC_ACCESSORY_TEST a where a.DIABETIC_FILE_NUMBER=t.DIABETIC_FILE_NUMBER and a.TEST_INSTITUTION_CODE=t.INSTITUTION_CODE_BUILDING_FILE)
where not exists(select 1 from DIABETIC_DRUG_RECORD a where a.DIABETIC_FILE_NUMBER=t.DIABETIC_FILE_NUMBER and a.MEDICAL_INSTITUTION_CODE=t.INSTITUTION_CODE_BUILDING_FILE)
where not exists(select 1 from DIABETIC_ACCESSORY_TEST a where a.FOLLOW_UP_NUMBER=t.FOLLOW_UP_NUMBER and a.TEST_INSTITUTION_CODE=t.FOLLOW_UP_INSTITUTION_CODE)
where not exists(select 1 from DIABETIC_DRUG_RECORD a where a.FOLLOW_UP_NUMBER=t.FOLLOW_UP_NUMBER and a.MEDICAL_INSTITUTION_CODE=t.FOLLOW_UP_INSTITUTION_CODE)
where not exists(select 1 from CANCER_FOLLOW_UP_RECORD a where a.CANCER_PATIENTS_FILE_NUMBER=t.CANCER_PATIENTS_FILE_NUMBER and a.FOLLOW_UP_INSTITUTION_CODE=t.INSTITUTION_CODE_BUILDING_FILE)
