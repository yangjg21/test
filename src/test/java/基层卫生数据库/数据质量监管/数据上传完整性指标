where not exists(select 1 from CANCER_ACCESSORY_TEST_RECORD a where a.CANCER_PATIENTS_FILE_NUMBER=t.CANCER_PATIENTS_FILE_NUMBER and a.TEST_INSTITUTION_CODE=t.INSTITUTION_CODE_BUILDING_FILE)
where not exists(select 1 from CANCER_PATIENTS_DRUG_RECORD a where a.CANCER_PATIENTS_FILE_NUMBER=t.CANCER_PATIENTS_FILE_NUMBER and a.MEDICAL_INSTITUTION_CODE=t.INSTITUTION_CODE_BUILDING_FILE)
where not exists(select 1 from CANCER_ACCESSORY_TEST_RECORD a where a.FOLLOW_UP_NUMBER=t.FOLLOW_UP_NUMBER and a.TEST_INSTITUTION_CODE=t.FOLLOW_UP_INSTITUTION_CODE)
where not exists(select 1 from CANCER_PATIENTS_DRUG_RECORD a where a.FOLLOW_UP_NUMBER=t.FOLLOW_UP_NUMBER and a.MEDICAL_INSTITUTION_CODE=t.FOLLOW_UP_INSTITUTION_CODE)
where not exists(select 1 from SCHIZOPHRENIA_FOLLOW_UP a where a.SCHIZOPHRENIA_FILE_NUMBER=t.SCHIZOPHRENIA_FILE_NUMBER and a.FOLLOW_UP_INSTITUTION_CODE=t.INSTITUTION_CODE_BUILDING_FILE)
where not exists(select 1 from SCHIZOPHRENIA_ACCESSORY_TEST a where a.SCHIZOPHRENIA_FILE_NUMBER=t.SCHIZOPHRENIA_FILE_NUMBER and a.TEST_INSTITUTION_CODE=t.INSTITUTION_CODE_BUILDING_FILE)
where not exists(select 1 from SCHIZOPHRENIA_DRUG_RECORD a where a.SCHIZOPHRENIA_FILE_NUMBER=t.SCHIZOPHRENIA_FILE_NUMBER and a.INSTITUTION_CODE=t.INSTITUTION_CODE_BUILDING_FILE)
where not exists(select 1 from SCHIZOPHRENIA_MED_AID a where a.SCHIZOPHRENIA_FILE_NUMBER=t.SCHIZOPHRENIA_FILE_NUMBER and a.MEDICAL_INSTITUTION_CODE=t.INSTITUTION_CODE_BUILDING_FILE)
where not exists(select 1 from SCHIZOPHRENIA_FAMILY_HIS a where a.SCHIZOPHRENIA_FILE_NUMBER=t.SCHIZOPHRENIA_FILE_NUMBER and a.MEDICAL_INSTITUTION_CODE=t.INSTITUTION_CODE_BUILDING_FILE)
where not exists(select 1 from SCHIZOPHRENIA_ACCESSORY_TEST a where a.FOLLOW_UP_NUMBER=t.FOLLOW_UP_NUMBER and a.TEST_INSTITUTION_CODE=t.FOLLOW_UP_INSTITUTION_CODE)
where not exists(select 1 from SCHIZOPHRENIA_DRUG_RECORD a where a.FOLLOW_UP_NUMBER=t.FOLLOW_UP_NUMBER and a.INSTITUTION_CODE=t.FOLLOW_UP_INSTITUTION_CODE)
where not exists(select 1 from GW038_MBGL_FJH_SF a where a.ZXDABH=t.ZXDABH and a.SFJG_DM=t.JDJG_DM)
where not exists(select 1 from THEOLD_FOLLOW_UP a where a.THE_OLD_FILE_NUMBER=t.THE_OLD_FILE_NUMBER and a.FOLLOW_UP_INSTITUTION_CODE=t.INSTITUTION_CODE_BUILDING_FILE)
where not exists(select 1 from THEOLD_ACCESSORY_TEST a where a.THE_OLD_FILE_NUMBER=t.THE_OLD_FILE_NUMBER and a.TEST_INSTITUTION_CODE=t.INSTITUTION_CODE_BUILDING_FILE)
where not exists(select 1 from THEOLD_DRUG_RECORD a where a.THE_OLD_FILE_NUMBER=t.THE_OLD_FILE_NUMBER and a.MEDICAL_INSTITUTION_CODE=t.INSTITUTION_CODE_BUILDING_FILE)
where not exists(select 1 from THEOLD_LIVING_ASSESS a where a.THE_OLD_FILE_NUMBER=t.THE_OLD_FILE_NUMBER and a.MEDICAL_INSTITUTION_CODE=t.INSTITUTION_CODE_BUILDING_FILE)
where not exists(select 1 from THEOLD_ACCESSORY_TEST a where a.FOLLOW_UP_NUMBER=t.FOLLOW_UP_NUMBER and a.TEST_INSTITUTION_CODE=t.FOLLOW_UP_INSTITUTION_CODE)
where not exists(select 1 from THEOLD_DRUG_RECORD a where a.FOLLOW_UP_NUMBER=t.FOLLOW_UP_NUMBER and a.MEDICAL_INSTITUTION_CODE=t.FOLLOW_UP_INSTITUTION_CODE)
where not exists(select 1 from DISABLED_FOLLOW_UP_RECORD a where a.DISABLED_FILE_NUMBER=t.DISABLED_FILE_NUMBER and a.FOLLOW_UP_INSTITUTION_CODE=t.INSTITUTION_CODE_BUILDING_FILE)
where not exists(select 1 from DISABLED_ACCESSORY_TEST a where a.DISABLED_FILE_NUMBER=t.DISABLED_FILE_NUMBER and a.TEST_INSTITUTION_CODE=t.INSTITUTION_CODE_BUILDING_FILE)
where not exists(select 1 from DISABLED_DRUG_RECORD a where a.DISABLED_FILE_NUMBER=t.DISABLED_FILE_NUMBER and a.MEDICAL_INSTITUTION_CODE=t.INSTITUTION_CODE_BUILDING_FILE)
where not exists(select 1 from DISABLED_ACCESSORY_TEST a where a.FOLLOW_UP_NUMBER=t.FOLLOW_UP_NUMBER and a.TEST_INSTITUTION_CODE=t.FOLLOW_UP_INSTITUTION_CODE)
where not exists(select 1 from DISABLED_DRUG_RECORD a where a.FOLLOW_UP_NUMBER=t.FOLLOW_UP_NUMBER and a.MEDICAL_INSTITUTION_CODE=t.FOLLOW_UP_INSTITUTION_CODE)
