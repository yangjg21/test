where not exists(select 1 from HYPERTENSION_DRUG_RECORD a where a.HYPERTENSION_FILE_NUMBER=t.HYPERTENSION_FILE_NUMBER and a.MEDICAL_INSTITUTION_CODE=t.INSTITUTION_CODE_BUILDING_FILE)
where not exists(select 1 from HYPERTENSION_GXYSQFZJL a where a.HYPERTENSION_FILE_NUMBER=t.CID and a.YLJGDM=t.INSTITUTION_CODE_BUILDING_FILE)
where not exists(select 1 from HYPERTENSION_ACCESSORY_TEST a where a.FOLLOW_UP_NUMBER=t.FOLLOW_UP_NUMBER and a.TEST_INSTITUTION_CODE=t.FOLLOW_UP_INSTITUTION_CODE)
where not exists(select 1 from HYPERTENSION_DRUG_RECORD a where a.FOLLOW_UP_NUMBER=t.FOLLOW_UP_NUMBER and a.MEDICAL_INSTITUTION_CODE=t.FOLLOW_UP_INSTITUTION_CODE)
