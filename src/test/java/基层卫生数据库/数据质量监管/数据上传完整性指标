where not exists(select 1 from TB_LIS_ALLERGY_RESULT a where a.BASIC_PERSONAL_INFO_NO=t.BASIC_PERSONAL_INFO_NO and a.RECORD_INSTITUTE_CODE=t.MEDICAL_INSTITUTION_CODE)
where not exists(select 1 from TB_LIS_ALLERGY_RESULT a where a.GRDAID=t.BASIC_PERSONAL_INFO_NO and a.YLJGDM=t.MEDICAL_INSTITUTION_CODE)
where not exists(select 1 from TB_LIS_ALLERGY_RESULT a where a.GRDAID=t.BASIC_PERSONAL_INFO_NO and a.YLJGDM=t.MEDICAL_INSTITUTION_CODE)
where not exists(select 1 from TB_LIS_ALLERGY_RESULT a where a.GRDAID=t.BASIC_PERSONAL_INFO_NO and a.YLJGDM=t.MEDICAL_INSTITUTION_CODE)
where not exists(select 1 from TB_LIS_ALLERGY_RESULT a where a.GRDAID=t.BASIC_PERSONAL_INFO_NO and a.YLJGDM=t.MEDICAL_INSTITUTION_CODE)
where not exists(select 1 from TB_LIS_ALLERGY_RESULT a where a.GRDAID=t.BASIC_PERSONAL_INFO_NO and a.YLJGDM=t.MEDICAL_INSTITUTION_CODE)
where not exists(select 1 from TB_LIS_ALLERGY_RESULT a where a.GRDAID=t.BASIC_PERSONAL_INFO_NO and a.YLJGDM=t.MEDICAL_INSTITUTION_CODE)
where not exists(select 1 from TB_LIS_ALLERGY_RESULT a where a.GRDAID=t.BASIC_PERSONAL_INFO_NO and a.YLJGDM=t.MEDICAL_INSTITUTION_CODE)
where not exists(select 1 from TB_LIS_ALLERGY_RESULT a where a.JKDABH=t.BASIC_PERSONAL_INFO_NO and a.DJJG_DM=t.MEDICAL_INSTITUTION_CODE)
where not exists(select 1 from TB_LIS_ALLERGY_RESULT a where a.JKDABH=t.BASIC_PERSONAL_INFO_NO and a.SFJG_DM=t.MEDICAL_INSTITUTION_CODE)
where not exists(select 1 from TB_LIS_ALLERGY_RESULT a where a.JKDABH=t.BASIC_PERSONAL_INFO_NO and a.SFJG_DM=t.MEDICAL_INSTITUTION_CODE)
where not exists(select 1 from TB_LIS_ALLERGY_RESULT a where a.HOUSEHOLDER_INFO_NO=t.BASIC_PERSONAL_INFO_NO and a.REGISTRATION_INSTITUTION_CODE=t.MEDICAL_INSTITUTION_CODE)
where not exists(select 1 from TB_LIS_ALLERGY_RESULT a where a.BASIC_PERSONAL_INFO_NO=t.BASIC_PERSONAL_INFO_NO and a.MEDICAL_INSTITUTION_CODE=t.MEDICAL_INSTITUTION_CODE)


